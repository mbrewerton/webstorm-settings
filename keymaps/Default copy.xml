<keymap version="1" name="Default copy" parent="$default">
  <action id="$Redo">
    <keyboard-shortcut first-keystroke="shift ctrl z" />
    <keyboard-shortcut first-keystroke="shift alt back_space" />
    <keyboard-shortcut first-keystroke="ctrl y" />
  </action>
  <action id="ActivateCommitToolWindow">
    <keyboard-shortcut first-keystroke="alt 0" />
    <keyboard-shortcut first-keystroke="shift alt 9" />
  </action>
  <action id="CopyElement" />
  <action id="Debug">
    <keyboard-shortcut first-keystroke="shift f9" />
    <keyboard-shortcut first-keystroke="f5" />
  </action>
  <action id="Editor Redo">
    <keyboard-shortcut first-keystroke="ctrl y" />
  </action>
  <action id="EditorAddOrRemoveCaret">
    <mouse-shortcut keystroke="shift alt button1" />
  </action>
  <action id="EditorAddRectangularSelectionOnMouseDrag">
    <mouse-shortcut keystroke="shift control alt button1" />
    <mouse-shortcut keystroke="alt button1" />
  </action>
  <action id="EditorCreateRectangularSelectionOnMouseDrag">
    <mouse-shortcut keystroke="button2" />
  </action>
  <action id="EditorDeleteLine" />
  <action id="GotoNextError" />
  <action id="QuickEvaluateExpression">
    <keyboard-shortcut first-keystroke="ctrl alt f8" />
  </action>
  <action id="RenameElement">
    <keyboard-shortcut first-keystroke="shift f6" />
    <keyboard-shortcut first-keystroke="f2" />
  </action>
  <action id="Replace">
    <keyboard-shortcut first-keystroke="ctrl h" />
  </action>
  <action id="ReplaceInPath">
    <keyboard-shortcut first-keystroke="shift ctrl h" />
  </action>
  <action id="Resume">
    <keyboard-shortcut first-keystroke="f9" />
    <keyboard-shortcut first-keystroke="f5" />
  </action>
  <action id="Run">
    <keyboard-shortcut first-keystroke="shift f10" />
    <keyboard-shortcut first-keystroke="alt f5" />
  </action>
  <action id="SplitVertically">
    <keyboard-shortcut first-keystroke="ctrl alt slash" />
  </action>
  <action id="StepInto">
    <keyboard-shortcut first-keystroke="f7" />
    <keyboard-shortcut first-keystroke="f11" />
  </action>
  <action id="StepOver">
    <keyboard-shortcut first-keystroke="f8" />
    <keyboard-shortcut first-keystroke="f10" />
  </action>
  <action id="ToggleBookmark" />
  <action id="ToggleFullScreen">
    <keyboard-shortcut first-keystroke="shift ctrl alt f" />
  </action>
  <action id="Vcs.UpdateProject">
    <keyboard-shortcut first-keystroke="ctrl t" />
    <keyboard-shortcut first-keystroke="ctrl alt w" />
  </action>
  <action id="angularCliGenerate">
    <keyboard-shortcut first-keystroke="shift ctrl alt a" />
  </action>
</keymap>